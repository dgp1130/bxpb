  "keywords": [
    "browser-extension",
    "browser-extensions",
    "chrome-extension",
    "chrome-extensions",
    "protocol-buffers",
    "library",
    "service",
    "services"
  ],
  "author": "Doug Parker <dgp1130@users.noreply.github.com>",
  "homepage": "https://github.com/dgp1130/bxpb#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgp1130/bxpb.git"
  },
  "bin": {
    "bxpb-protoc-plugin": "bin/bxpb-protoc-plugin"
  },
  "scripts": {
